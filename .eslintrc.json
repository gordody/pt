{
    "extends": "standard",
    "env": {
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "rules": {
        "semi": ["error", "always"],
        "space-before-function-paren": ["error", "never"],
        "no-multi-spaces": ["error", "never"],
        "array-bracket-spacing": ["error", "always"]
    }
}